{
  "name": "@amzn/sql-context-presets",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "ssh://git.amazon.com/pkg/Sql-context-presets"
  },
  "homepage": "https://code.amazon.com/packages/Sql-context-presets",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules",
    "build": "tsc",
    "format": "prettier --write --cache .",
    "lint": "eslint --fix --cache src",
    "pretest": "npm run format && npm run lint",
    "inspector": "mcp-inspector",
    "prepublishOnly": "npm run build && npm run test",
    "test": "vitest --run ./test/ --coverage",
    "integ": "vitest --run ./integ/ --coverage",
    "posttest": "generate-coverage-data --language typescript",
    "toolbox:bundle": "BUNDLE_ONLY=true tsx scripts/bundle-and-publish-local.ts",
    "toolbox:publish-head": "TOOLBOX_CHANNEL=head tsx scripts/bundle-and-publish-local.ts",
    "toolbox:publish-stable": "TOOLBOX_CHANNEL=stable tsx scripts/bundle-and-publish-local.ts"
  },
  "files": [
    "dist/"
  ],
  "npm-pretty-much": {
    "runTest": "never",
    "appName": "sql-context-presets"
  },
  "bin": {
    "sql-context-presets": "dist/index.js"
  },
  "dependencies": {
    "@amzn/amazon-ca-certs": "*",
    "@amzn/midway-client": "*",
    "@amzn/midway-client-suite-library-node-js": "*",
    "@aws-sdk/client-s3": "^3.700.0",
    "@modelcontextprotocol/sdk": "^1.10.2",
    "axios": "^1.8.4",
    "dedent-js": "^1.0.1",
    "find-up-simple": "^1.0.1"
  },
  "devDependencies": {
    "@amzn/brazil": "^2.0.6",
    "@eslint/js": "^9.26.0",
    "@modelcontextprotocol/inspector": "^0.10.2",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.1.3",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "ts-invariant": "^0.10.3",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.32.0",
    "uuid": "^11.1.0",
    "vitest": "^3.1.3"
  }
}
